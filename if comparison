import java.util.Scanner;

public class Comparison{
	
	public static void main(String[] args){
		
		int num1;
		int num2;
		
		Scanner input = new Scanner(System.in);
		System.out.print("enter a num1");
		num1=input.nextInt() ;
		System.out.print("enter a num2");
		num2=input.nextInt() ;
		
		if(num1==num2)
		System.out.print("both thee numbers r equal");

        		if(num1>num2)
		System.out.print("num 1 is greater than num2");
		
		
        		if(num1<num2)
		System.out.print("num 2 is greater than num1");
		
		
	}
}
