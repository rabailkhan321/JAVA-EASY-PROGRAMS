import java.util.Scanner;

 
public class Welcome{
	
	public static void main(String[] args){
		Scanner input= new Scanner(System.in);
		
		int num1;
		int num2;
		int sum=0;
		System.out.print("enter num 1:");
		num1=input.nextInt();
		System.out.print("enter num 1:");
		num2=input.nextInt();
		
		sum = num1+num2;
		System.out.printf("sum is %d", sum);
	}
}
	
